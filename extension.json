	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "NewSignupPage"
	},
	"ResourceModules": {
		"ext.newsignuppage": {
			"scripts": "resources/js/NewSignupPage.js",
			"messages": [
				"badretype"
			],
			"position": "bottom"
