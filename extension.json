{
	"name": "New Signup Page",
	"version": "1.1.0",
	"author": [
		"Jack Phoenix"
	],
	"license-name": "GPL-2.0-or-later",
	"url": "https://www.mediawiki.org/wiki/Extension:NewSignupPage",
	"descriptionmsg": "newsignuppage-desc",
	"type": "other",
	"config": {
		"RegisterTrack": false,
		"AutoAddFriendOnInvite": false,
		"ForceNewSignupPageInitialization": false,
		"NspForceAnonEditExplicitAccept": true,
		"NspExplicitAddToAcceptGroup": true
	},
	"MessagesDirs": {
		"NewSignupPage": [
			"i18n"
		]
	},
	"AutoloadClasses": {
		"NewSignupPage": "includes/NewSignupPage.class.php",
		"NewSignupPageAuthenticationRequest": "includes/auth/NewSignupPageAuthenticationRequest.php",
		"NewSignupPageSecondaryAuthenticationProvider": "includes/auth/NewSignupPageSecondaryAuthenticationProvider.php",
		"PrivacySettingsExt": "includes/PrivacySettings.class.php"
	},
	"SpecialPages": {
		"privacy-settings": "PrivacySettingsExt"
	},
	"AuthManagerAutoConfig": {
		"secondaryauth": {
			"NewSignupPageSecondaryAuthenticationProvider": {
				"class": "NewSignupPageSecondaryAuthenticationProvider"
			}
		}
	},
	"Hooks": {
		"BeforePageDisplay": "NewSignupPage::onBeforePageDisplay",
		"EditPageBeforeEditButtons": "NewSignupPage::onEditPageBeforeEditButtons",
		"EditFilter": "NewSignupPage::onEditFilter",
		"LocalUserCreated": "NewSignupPage::onLocalUserCreated"
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "NewSignupPage"
	},
	"ResourceModules": {
		"ext.newsignuppage": {
			"scripts": "resources/js/NewSignupPage.js",
			"messages": [
				"badretype"
			]
		}
	},
	"AvailableRights": [
		"bypasstoscheck",
		"TOS Accepted",
		"TOS Rejected"
	],
	"manifest_version": 1
}
